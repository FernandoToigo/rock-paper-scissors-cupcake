{
	"scripts": {
		"test": "nyc ava src/tests/**/*.js --verbose",
		"start": "webpack-dev-server --open"
	},
	"devDependencies": {
		"@ava/babel-preset-stage-4": "^1.1.0",
		"ava": "1.0.0-beta.6",
		"babel-register": "^6.26.0",
		"nyc": "^12.0.2",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.5"
	},
	"ava": {
		"require": [
			"babel-register"
		]
	},
	"nyc": {
		"exclude": [
			"src/tests**"
		],
		"include": [
			"src/**/*.js"
		],
		"all": true
	},
	"dependencies": {
		"all": "0.0.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-preset-env": "^1.7.0"
	}
}
